generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Division {
  id        Int     @id @default(autoincrement())
  divId     String? @unique @map("DIV_ID") @db.VarChar(7)
  divName   String? @map("DIV_NAME") @db.VarChar(100)
  divNameMM String? @map("DIV_NAMEMM") @db.VarChar(225)

  township Township[]

  @@map("tbl_division")
}

model Township {
  id           Int     @id @default(autoincrement())
  tspId        String? @unique @map("TSP_ID") @db.VarChar(100)
  tspName      String? @map("TSP_NAME") @db.VarChar(100)
  tspShortName String? @map("TSP_SHORTNAME") @db.VarChar(100)
  divId        String? @map("DIV_ID") @db.VarChar(50)
  tspLat       Float?  @map("TSP_LAT") @db.Double
  tspLong      Float?  @map("TSP_LONG") @db.Double

  villages Village[]

  division Division? @relation(fields: [divId], references: [divId])

  @@map("tbl_township")
}

model Village {
  id               Int     @id @default(autoincrement())
  villageCode      String? @unique @map("VILLAGE_CODE") @db.VarChar(20)
  villageName      String? @map("VILLAGE_NAME") @db.VarChar(100)
  villageNameMM    String? @map("VILLAGE_NAMEMM") @db.VarChar(100)
  villageMalePop   Int?    @default(0) @map("VILLAGE_MALEPOP")
  villageFemalePop Int?    @default(0) @map("VILLAGE_FEMALEPOP")
  villageLatitude  Float?  @default(0.0) @map("VILLAGE_LATITUDE") @db.Double
  villageLongitude Float?  @default(0.0) @map("VILLAGE_LONGITUDE") @db.Double
  villageHousehold Int?    @default(0) @map("VILLAGE_HOUSEHOLD")
  tspId            String? @map("TSP_ID") @db.VarChar(100)
  tspIdeho         String? @map("TSP_IDEHO") @db.VarChar(100)
  villageStatus    Int?    @default(0) @map("VILLAGE_STATUS")

  patients    Patient[]
  cfrms       Cfrm[]
  township    Township?    @relation(fields: [tspId], references: [tspId])
    // Define relationship to VillageOrgProj
  villageOrgProjs  VillageOrgProj[]

  @@map("tbl_village")
}

model Clinic {
  id      Int     @id @default(autoincrement())
  clnId   String? @unique @map("CLN_ID") @db.VarChar(100)
  clnName String? @map("CLN_NAME") @db.VarChar(100)
  clnTsp  String? @map("CLN_TSP") @db.VarChar(100)
  ehoTsp  String? @map("EHO_TSP") @db.VarChar(50)
  clnPop  Int?    @default(0) @map("CLN_POP") @db.Int

  ancs      Anc[]
  deliverys Delivery[]
  pncs      Pnc[]
  fps       Fp[]
  rhs       Rh[]
  gms       Gm[]

  @@map("TBL_CLINIC")
}

model Org {
  id           Int     @id @default(autoincrement()) @map("ID")
  orgId        String? @unique @map("ORG_ID") @db.VarChar(10)
  orgName      String? @map("ORG_NAME") @db.VarChar(100)
  orgShortName String? @map("ORG_SHORTNAME") @db.VarChar(100)

  patients  Patient[]
  users     User[]
  ancs      Anc[]
  deliverys Delivery[]
  pncs      Pnc[]
  fps       Fp[]
  rhs       Rh[]
  gms       Gm[]
  cfrms     Cfrm[]

  @@map("TBL_ORG")
}

model Project {
  projectId          String    @id @map("PROJECT_ID") @db.VarChar(10)
  projectName        String    @map("PROJECT_NAME") @db.VarChar(100)
  donorId            String?   @map("DONOR_ID") @db.VarChar(10)
  projectStartDate   DateTime? @map("PROJECT_STARTDATE") @db.Date
  projectEndDate     DateTime? @map("PROJECT_ENDDATE") @db.Date
  projectDescription String?   @map("PROJECT_DESCRIPTION") @db.VarChar(255)
  projectStatus      Int?      @map("PROJECT_STATUS") @db.Int

  cfrms Cfrm[]

  @@map("TBL_PROJECT")
}

model Donor {
  donorId        String    @id @map("DONOR_ID") @db.VarChar(10)
  donorName      String?   @map("DONOR_NAME") @db.VarChar(100)
  donorShortName String?   @map("DONOR_SHORTNAME") @db.VarChar(10)
  grantName      String?   @map("GRANT_NAME") @db.VarChar(45)
  startDate      DateTime? @map("START_DATE") @db.Date
  endDate        DateTime? @map("END_DATE") @db.Date

  @@map("TBL_DONOR")
}

model User {
  userId       Int       @id @default(autoincrement()) @map("USER_ID") @db.Int
  userFullName String?   @map("USER_FULL_NAME") @db.VarChar(100)
  userOrg      String?   @map("USER_ORG") @db.VarChar(100)
  userName     String?   @unique @map("USER_NAME") @db.VarChar(100)
  userType     String?   @map("USER_TYPE") @db.VarChar(100)
  userPassword String?   @map("USER_PASSWORD") @db.VarChar(1000)
  createdBy    String?   @map("CREATED_BY") @db.VarChar(100)
  createdDate  DateTime? @map("CREATED_DATE") @db.Date
  userStatus   Int?      @map("USER_STATUS") @db.Int
  userSalt     String?   @map("USER_SALT") @db.VarChar(20)
  userExpired  DateTime? @map("USER_EXPIRED") @db.Date
  userCln      String?   @map("USER_CLN") @db.VarChar(20)
  role         Role?     @relation(fields: [roleId], references: [id])
  roleId       Int?

  organization Org? @relation(fields: [userOrg], references: [orgId])

  @@map("TBL_USER")
}

model District {
  id                Int     @id @default(autoincrement()) @map("ID")
  knuDistrictCode   String? @map("KNU_District_Code")
  knuDistrictNameEN String? @map("KNU_District_Name_EN")

  @@map("TBL_DISTRICT")
}

model EHOTownship {
  id                Int     @id @default(autoincrement()) @map("ID")
  knuDistrictCode   String? @map("KNU_District_Code")
  knuDistrictNameEN String? @map("KNU_District_Name_EN")
  townshipId        String? @unique @map("Township_ID")
  townshipName      String? @map("Township_Name")

  @@map("TBL_EHOTOWNSHIP")
}

model Role {
  id          Int          @id @default(autoincrement())
  name        String       @unique
  permissions Permission[]
  users       User[]
}

model Permission {
  id    Int    @id @default(autoincrement())
  name  String @unique
  roles Role[]
}

model Patient {
  id            Int       @id @default(autoincrement()) @map("ID")
  regId         String    @unique @map("REGID") @db.VarChar(20)
  regName       String    @map("REGNAME") @db.VarChar(100)
  regDate       DateTime  @map("REGDATE") @db.Date
  regOrg        String    @map("REGORG") @db.VarChar(10)
  regPlace      Int       @default(999) @map("REGPLACE") @db.Int
  regVillage    String    @map("REGVILLAGE") @db.VarChar(20)
  regAge        Int       @default(999) @map("REGAGE") @db.Int
  regAgeUnit    Int       @default(999) @map("REGAGEUNIT") @db.Int
  regSex        Int       @default(999) @map("REGSEX") @db.Int
  regType       Int       @default(999) @map("REGTYPE") @db.Int
  regEdu        Int       @default(999) @map("REGEDU") @db.Int
  regJob        String?   @map("REGJOB") @db.VarChar(30)
  regMarital    Int?      @default(999) @map("REGMARITAL") @db.Int
  regSpouse     String?   @map("REGSPOUSE") @db.VarChar(30)
  regMother     String?   @map("REGMOTHER") @db.VarChar(30)
  regFather     String?   @map("REGFATHER") @db.VarChar(30)
  regAddress    String?   @map("REGADDRESS") @db.VarChar(100)
  regPh         String?   @map("REGPH") @db.VarChar(30)
  regEthnic     String?   @map("REGETHNIC") @db.VarChar(100)
  regRefFrom    String?   @map("REGREFFROM") @db.VarChar(1000)
  regRemark     String?   @map("REGREMARK") @db.VarChar(255)
  regUsrLogin   String?   @map("REGUSRLOGIN") @db.VarChar(100)
  regInsert     DateTime? @map("REGINSERT") @db.Timestamp(6)
  regUpdate     DateTime? @map("REGUPDATE") @db.Timestamp(6)
  regStatus     Int?      @default(0) @map("REGSTATUS") @db.Int
  regSync       Int?      @default(0) @map("REGSYNC") @db.Int
  regMigrant    Int?      @default(999) @map("REGMIGRANT") @db.Int
  regIdp        Int?      @default(999) @map("REGIDP") @db.Int
  regDsee       Int?      @default(999) @map("REGDSEE") @db.Int
  regDhear      Int?      @default(999) @map("REGDHEAR") @db.Int
  regDwalk      Int?      @default(999) @map("REGDWALK") @db.Int
  regDrembr     Int?      @default(999) @map("REGDREMBR") @db.Int
  regDwash      Int?      @default(999) @map("REGDWASH") @db.Int
  regDcommu     Int?      @default(999) @map("REGDCOMMU") @db.Int
  regEthnicO    String?   @map("REGETHNICO") @db.VarChar(100)
  regDisability Int?      @default(999) @map("REGDISABILITY") @db.Int

  village      Village? @relation(fields: [regVillage], references: [villageCode])
  organization Org?     @relation(fields: [regOrg], references: [orgId])

  ancs      Anc[]
  labs      Lab[]
  deliverys Delivery[]
  pncs      Pnc[]
  fps       Fp[]
  rhs       Rh[]
  gms       Gm[]

  @@map("TBL_REG")
}

model Anc {
  id                     Int       @id @default(autoincrement()) @map("ID")
  anRegId                String?   @map("ANREGID") @db.VarChar(20)
  anProvidedDate         DateTime? @map("ANPROVIDEDDATE") @db.Date
  anType                 Int?      @default(999) @map("ANTYPE")
  anDonor                String?   @map("ANDONOR") @db.VarChar(10)
  anOrg                  String?   @map("ANORG") @db.VarChar(10)
  anProject              String?   @map("ANPROJECT") @db.VarChar(10)
  anTsp                  String?   @map("ANTSP") @db.VarChar(100)
  anPlace                Int?      @default(999) @map("ANPLACE")
  anVillage              String?   @map("ANVILLAGE") @db.VarChar(20)
  anProviderName         String?   @map("ANPROVIDERNAME") @db.VarChar(30)
  anProviderPosition     Int?      @default(999) @map("ANPROVIDERPOSITION")
  anUsrLogin             String?   @map("ANUSRLOGIN") @db.VarChar(100)
  anLmp                  DateTime? @map("ANLMP") @db.Date
  anEdd                  DateTime? @map("ANEDD") @db.Date
