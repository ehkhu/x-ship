  anFeSo4                Int?      @default(999) @map("ANFESO4")
  anFc                   Int?      @default(999) @map("ANFC")
  anB1                   Int?      @default(999) @map("ANB1")
  anDeworm1              Int?      @default(999) @map("ANDEWORM1")
  anTt1                  Int?      @default(999) @map("ANTT1")
  anCdk                  Int?      @default(999) @map("ANCDK")
  anNbk                  Int?      @default(999) @map("ANNBK")
  anDirectComplication   Int?      @default(999) @map("ANDIRECTCOMPLICATION")
  anIndirectComplication Int?      @default(999) @map("ANINDIRECTCOMPLICATION")
  anIndirectOther        String?   @map("ANINDIRECTOTHER") @db.VarChar(1000)
  anIndirectDx           String?   @map("ANINDIRECTDX") @db.VarChar(1000)
  anIndirectTx           String?   @map("ANINDIRECTTX") @db.VarChar(1000)
  anHe1                  Int?      @default(999) @map("ANHE1")
  anHe2                  Int?      @default(999) @map("ANHE2")
  anHe3                  Int?      @default(999) @map("ANHE3")
  anHe4                  Int?      @default(999) @map("ANHE4")
  anHe5                  Int?      @default(999) @map("ANHE5")
  anHe6                  Int?      @default(999) @map("ANHE6")
  anHe7                  Int?      @default(999) @map("ANHE7")
  anHe8                  Int?      @default(999) @map("ANHE8")
  anOutcome              Int?      @default(999) @map("ANOUTCOME")
  anRefTo                Int?      @default(999) @map("ANREFTO")
  anRefToOther           String?   @map("ANREFTOOTHER") @db.VarChar(1000)
  anRefReason            String?   @map("ANREFREASON") @db.VarChar(1000)
  anDeathReason          String?   @map("ANDEATHREASON") @db.VarChar(1000)
  anVisit                Int?      @default(999) @map("ANVISIT")
  anHe9                  Int?      @default(999) @map("ANHE9")
  anHe10                 Int?      @default(999) @map("ANHE10")
  anHe11                 Int?      @default(999) @map("ANHE11")
  anHe12                 Int?      @default(999) @map("ANHE12")
