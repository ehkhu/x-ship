  deliDeliType         Int?      @default(999) @map("DELIDELITYPE")
  deliGp               Int?      @default(999) @map("DELIGP")
  deliG                Int?      @default(999) @map("DELIG")
  deliP                Int?      @default(999) @map("DELIP")
  deliA                Int?      @default(999) @map("DELIA")
  deliEpi              Int?      @default(999) @map("DELIEPI")
  deliDefect           Int?      @default(999) @map("DELIDEFECT")
  deliPN6              Int?      @default(999) @map("DELIPN6")
  deliLab              Int?      @default(0) @map("DELILAB")
