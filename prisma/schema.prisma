  anBp                   String?   @default("000/00") @map("ANBP") @db.VarChar(10)
  anPr                   Int?      @default(999) @map("ANPR")
  anRr                   Int?      @default(999) @map("ANRR")
  anTemp                 Float?    @default(999.90) @map("ANTEMP") @db.Float
  anGp                   Int?      @default(999) @map("ANGP")
  anEdema                Int?      @default(999) @map("ANODEMA")
  anNoFetus              Int?      @default(999) @map("ANNOFETUS")
  anLie                  Int?      @default(999) @map("ANLIE")
  anPresentation         Int?      @default(999) @map("ANPRESENTATION")
  anFundalHt             Int?      @default(999) @map("ANFUNDALHT")
  anFhs                  Int?      @default(999) @map("ANFHS")
  anOther                String?   @map("ANOTHER") @db.VarChar(1000)
  anLab                  Int?      @default(0) @map("ANLAB")
