  anFirstPreg            Int?      @default(999) @map("ANFIRSTPREG")
  anHygKit               Int?      @default(999) @map("ANHYGKIT")
  anB1Unit               Int?      @default(999) @map("ANB1UNIT")
  anAge                  Int?      @default(999) @map("ANAGE")
  anAgeUnit              Int?      @default(999) @map("ANAGEUNIT")
  anVtCount              Int?      @default(999) @map("ANVTCOUNT")
  anTempUnit             Int?      @default(999) @map("ANTEMPUNIT")
  anVisitSkill           Int?      @default(999) @map("ANVISITSKILL")
  anVisitTimingSkill     Int?      @default(999) @map("ANVISITTIMINGSKILL")
  anDeworm2              Int?      @default(999) @map("ANDEWORM2")
  anTt2                  Int?      @default(999) @map("ANTT2")
  anClnId                String?   @map("ANCLNID") @db.VarChar(10)
  anInsert               DateTime? @map("ANINSERT") @db.Timestamp(6)
  anUpdate               DateTime? @map("ANUPDATE") @db.Timestamp(6)
  anStatus               Int?      @default(0) @map("ANSTATUS")
  anSync                 Int?      @default(0) @map("ANSYNC")
  anVisitTiming          Int?      @map("ANVISITTIMING")
  anType2                Int?      @default(999) @map("ANTYPE2")
  anMigrant              Int?      @default(999) @map("ANMIGRANT")
  anIdp                  Int?      @default(999) @map("ANIDP")
  anDsee                 Int?      @default(999) @map("ANDSEE")
  anDhear                Int?      @default(999) @map("ANDHEAR")
  anDwalk                Int?      @default(999) @map("ANDWALK")
  anDrembr               Int?      @default(999) @map("ANDREMBR")
  anDwash                Int?      @default(999) @map("ANDWASH")
  anDcommu               Int?      @default(999) @map("ANDCOMMU")
  anDisability           Int?      @default(999) @map("ANDISABILITY")
  anFaFeSo4              Float?    @default(999) @map("ANFAFESO4") @db.Float
  patient                Patient?  @relation(fields: [anRegId], references: [regId])
  clinic                 Clinic?   @relation(fields: [anClnId], references: [clnId])
  organization           Org?      @relation(fields: [anOrg], references: [orgId])

  @@map("TBL_ANC")
}

model Lab {
  id              Int       @id @default(autoincrement()) @map("ID")
  labRegId        String?   @map("LABREGID") @db.VarChar(20)
  labProvidedDate DateTime? @map("LABPROVIDEDDATE") @db.Date
  labPlace        Int?      @default(999) @map("LABPLACE")
  labVillage      String?   @map("LABVILLAGE") @db.VarChar(20)
  labRdt          Int?      @default(999) @map("LABRDT")
  labMicroscopic  Int?      @default(999) @map("LABMICROSCOPIC")
  labHb           Int?      @default(999) @map("LABHB")
  labBg           Int?      @default(999) @map("LABBG")
  labRh           Int?      @default(999) @map("LABRH")
  labUcg          Int?      @default(999) @map("LABUCG")
  labUSugar       Int?      @default(999) @map("LABUSUGAR")
  labUProtein     Int?      @default(999) @map("LABUPROTEIN")
  labGono         Int?      @default(999) @map("LABGONO")
  labTricho       Int?      @default(999) @map("LABTRICHO")
  labCandida      Int?      @default(999) @map("LABCANDIDA")
  labRpr          Int?      @default(999) @map("LABRPR")
  labTpha         Int?      @default(999) @map("LABTPHA")
  labVdrl         Int?      @default(999) @map("LABVDRL")
  labHiv          Int?      @default(999) @map("LABHIV")
  labHbv          Int?      @default(999) @map("LABHBV")
  labHcv          Int?      @default(999) @map("LABHCV")
  labSsource      String?   @map("LABSSOURCE") @db.VarChar(20)
  labOther        String?   @map("LABOTHER") @db.VarChar(1000)
  labRbs          Float?    @default(0.0) @map("LABRBS") @db.Float
  labOrg          String?   @map("LABORG") @db.VarChar(10)
  labInsert       DateTime? @map("LABINSERT") @db.Timestamp(6)
  labUpdate       DateTime? @map("LABUPDATE") @db.Timestamp(6)
  labStatus       Int?      @default(0) @map("LABSTATUS")
  labSync         Int?      @default(0) @map("LABSYNC")
  labTest         Int?      @default(0) @map("LABTEST")

  patient Patient? @relation(fields: [labRegId], references: [regId])

  @@unique([id, labRegId, labProvidedDate, labSsource]) // Composite unique constraint
  @@map("TBL_LAB")
}

model VillageOrgProj {
  id          Int       @id @default(autoincrement()) @map("ID")
  villageCode String?   @map("VILLAGE_CODE") @db.VarChar(20)
  orgCode     String?   @map("ORG_CODE") @db.VarChar(10)
  projCode    String?   @map("PROJ_CODE") @db.VarChar(10)
  tspCode     String?   @map("TSP_CODE") @db.VarChar(20)
  clnCode     String?   @map("CLN_CODE") @db.VarChar(20)
  vopStatus   Int?      @default(0) @map("VOP_STATUS") // BIGINT to Int
  vopInsert   DateTime? @map("VOP_INSERT") @db.Timestamp(6)
  vopUpdate   DateTime? @map("VOP_UPDATE") @db.Timestamp(6)
  vopRemark   String?   @map("VOP_REMARK") @db.VarChar(255)
  vopVlt      Int?      @default(0) @map("VOP_VLT") // BIGINT to Int
  vopInfomx   Int?      @default(0) @map("VOP_INFOMX") // BIGINT to Int
  vopMpop     Int?      @map("VOP_MPOP") // BIGINT to Int
  vopFpop     Int?      @map("VOP_FPOP") // BIGINT to Int
  vopHhold    Int?      @map("VOP_HHOLD") // BIGINT to Int
  // Define relationship to Village
  village     Village?  @relation(fields: [villageCode], references: [villageCode])
  @@map("TBL_VILLAGE_ORG_PROJ") // Map model to table name
}

